    "\n",
    "masculino_idade40_sem_doenca = sem_doenca_cardiaca.query(\"Age < 40 and Sex == 'M'\")\n",
    "masculino_idade40_50_sem_doenca = sem_doenca_cardiaca.query(\"Age >= 40 and Age < 50 and Sex == 'M'\")\n",
