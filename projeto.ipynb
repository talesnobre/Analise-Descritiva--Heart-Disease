    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "import seaborn as sns\n",
    "import plotly.express as px\n",
    "sns.set()\n",
    "\n",
    "arquivo = pd.read_csv('heart.csv')\n",
    "\n",
    "dados = pd.DataFrame(arquivo, copy=True)\n",
    "\n",
    "doenca_cardiaca = dados.query('HeartDisease == 1')\n",
    "sem_doenca_cardiaca = dados.query('HeartDisease == 0')\n",
    "\n",
    "##### SEPARAÇÃO POR IDADES COM E SEM DOENÇA CARDÍACA\n",
    "\n",
    "idade40_com_doenca = doenca_cardiaca.query('Age < 40')\n",
    "idade40_50_com_doenca = doenca_cardiaca.query('Age >= 40 and Age < 50')\n",
    "idade50_60_com_doenca = doenca_cardiaca.query('Age >= 50 and Age < 60')\n",
    "idade60_70_com_doenca = doenca_cardiaca.query('Age >= 60 and Age < 70')\n",
    "idade70_com_doenca = doenca_cardiaca.query('Age > 70')\n",
    "\n",
    "idade40_sem_doenca = sem_doenca_cardiaca.query('Age < 40')\n",
    "idade40_50_sem_doenca = sem_doenca_cardiaca.query('Age >= 40 and Age < 50')\n",
    "idade50_60_sem_doenca = sem_doenca_cardiaca.query('Age >= 50 and Age < 60')\n",
    "idade60_70_sem_doenca = sem_doenca_cardiaca.query('Age >= 60 and Age < 70')\n",
    "idade70_sem_doenca = sem_doenca_cardiaca.query('Age > 70')\n",
