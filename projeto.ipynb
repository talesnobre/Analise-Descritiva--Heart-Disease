    "feminino_idade70_com_doenca = doenca_cardiaca.query(\"Age > 70 and Sex == 'F'\")\n",
    "\n",
    "feminino_idade40_sem_doenca = sem_doenca_cardiaca.query(\"Age < 40 and Sex == 'F'\")\n",
